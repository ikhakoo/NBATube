<h1>Add a Video</h1>
<%= form_tag "/tubes" do %>
  <p>
    <%= label_tag :source %><br>
    <%= text_field_tag :source %>
  </p>
  <p>
    <%= label_tag :title %><br>
    <%= text_field_tag :title %>
  </p>
  <p>
    <%= label_tag :url %><br>
    <%= text_field_tag :url %>
  </p>
  <p>
    <%= submit_tag "Save" %>
  </p>
<% end %>